		{
			"name": "Accessor",
			"scope": [
				"punctuation.accessor"
			],
			"settings": {
				"foreground": "#f29e74"
			}
		},
		{
			"name": "Interpolation text",
			"scope": [
				"meta.embedded"
			],
			"settings": {
				"foreground": "#cccac2"
			}
		},
		{
			"name": "Inherited class type",
			"scope": [
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#5ccfe6"
			}
		},
		{
			"name": "Lambda arrow",
			"scope": [
				"storage.type.function"
			],
			"settings": {
				"foreground": "#ffad66"
			}
		},
