{
	"$schema": "vscode://schemas/color-theme",
	"type": "dark",
	"include": "./gray_vs_only.json",
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#b8cfe680"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"constant.other.symbol"
			],
			"settings": {
				"foreground": "#b8cc52"
			}
		},
		{
			"name": "Regular Expressions and Escape Characters",
			"scope": [
				"string.regexp",
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#95e6cb"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#dfbfff"
			}
		},
		{
			"name": "Built-in constants",
			"scope": [
				"constant.language"
			],
			"settings": {
				"foreground": "#dfbfff"
			}
		},
		// {
		// 	"name": "Variable",
